
#include <bits/stdc++.h>
#define fast ios_base::sync_with_stdio(0);cin.tie(0);
using namespace std;

typedef pair<int,int> pii;
typedef long long ll;
const int INF=99999999;

const int N=16;
ll dp[11][2001];

int n,m;

ll find(int n,int m)
{
	
	if(m==0) return 0;
	if(n==1) return m;
	
	if(dp[n][m]!=-1) return dp[n][m];
	
	
	ll ch1=find(n,m-1);
  
  // tricky state if we add m/2 --> 1 then i will we redudant counting
	ll ch2=find(n-1,m/2);
	
	return dp[n][m]=ch1+ch2;
}


int main()
{
	
	int test;
	scanf("%d",&test);
	for(int t=1;t<=test;t++)
	{
		memset(dp,-1,sizeof(dp));
		int n,m;
		scanf("%d%d",&n,&m);
		printf("Data set %d: %d %d %lld\n",t,n,m,find(n,m));
	}
	return 0;
}
